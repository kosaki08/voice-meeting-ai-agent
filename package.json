{
  "name": "voice-meeting-ai-agent",
  "version": "1.0.0",
  "type": "module",
  "description": "Voice Meeting AI Agent",
  "main": "index.js",
  "exports": {
    "./*": "./dist/*"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:integration": "jest --config jest.integration.config.ts --runInBand",
    "test:integration:ci": "CI=true jest --config jest.integration.config.ts --runInBand",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write --ignore-unknown .",
    "example:audio": "tsx examples/audio-reception-demo.ts",
    "example:transcription": "tsx examples/transcription-demo.ts"
  },
  "keywords": [],
  "author": "Kazunori Osaki",
  "license": "MIT",
  "packageManager": "pnpm@10.6.4",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "discord.js": "^14.19.3",
    "got": "^14.4.7",
    "libsodium-wrappers": "^0.7.15",
    "openai": "^5.3.0",
    "prism-media": "^1.3.5",
    "uuid": "^11.1.0",
    "zod": "^3.25.64"
  },
  "optionalDependencies": {
    "opusscript": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.13.5",
    "eslint-plugin-prettier": "^5.4.1",
    "ffmpeg-static": "5.2.0",
    "globals": "^16.2.0",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.7",
    "nock": "^14.0.5",
    "prettier": "^3.5.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
